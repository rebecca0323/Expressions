<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.livewell2020">

    <library tools:node="removeAll" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.Light.NoActionBar">
        <activity android:name=".Level2"></activity>
        <activity android:name=".Congrats">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Statistics" />
        <activity android:name=".Incorrect" />
        <activity android:name=".Correct" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity android:name=".Camera" />
        <activity android:name=".Game" />
        <activity android:name=".Instructions" />
        <activity android:name=".Song" />
        <activity android:name=".Playlist" />
        <activity android:name=".Register" />
        <activity android:name=".Login" />
        <activity android:name=".Splash"></activity>
    </application>

</manifest>